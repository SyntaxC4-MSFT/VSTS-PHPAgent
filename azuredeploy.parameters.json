{
	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"location": {
			"value": "West US"
		},
		"username": {
			"value": "youruser"
		},
		"password": {
			"value": "yourpasswd"
		},
		"newStorageAccountName": {
			"value": "uniquestorageaccount"
		},
		"vmSize": {
			"value": "Standard_D1"
		},
		"dnsNameForPublicIP": {
			"value": "uniquednsname"
		},
		"fileUris": {
			"value": "https://github.com/SyntaxC4-MSFT/VSTS-PHPAgent/raw/master/scripts/bootstrap.sh https://github.com/SyntaxC4-MSFT/VSTS-PHPAgent/raw/master/scripts/install-agent.sh https://github.com/SyntaxC4-MSFT/VSTS-PHPAgent/raw/master/scripts/install-dep.sh https://github.com/SyntaxC4-MSFT/VSTS-PHPAgent/raw/master/scripts/update-apt.sh"
		},
		"commandToExecute": {
			"value": "./bootstrap.sh"
		},
    "vstsUsername": {
      "value": "yourvstsuser"
